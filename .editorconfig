# http://editorconfig.org/
# top-most EditorConfig file
root = true

[*]
end_of_line = lf
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.cmd]
end_of_line = crlf

[*.yml]
indent_style = space
indent_size = 2

[app/help.txt]
indent_style = space
indent_size = 2
